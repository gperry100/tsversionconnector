<?xml version="1.0" encoding="UTF-8"?>

<mule xmlns="http://www.mulesoft.org/schema/mule/core"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xmlns:spring="http://www.springframework.org/schema/beans"
      xmlns:ts-version="http://www.mulesoft.org/schema/mule/ts-version"
      xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
        http://www.mulesoft.org/schema/mule/core http://www.mulesoft.org/schema/mule/core/current/mule.xsd
        http://www.mulesoft.org/schema/mule/ts-version http://www.mulesoft.org/schema/mule/ts-version/1.0/mule-ts-version.xsd">

    <ts-version:config-type name="config" greeting="Hi"/>

    <flow name="testFlow">
        <ts-version:greet config-ref="config" friend="Foo"/>
    </flow>
    <flow name="testAddEntiy">
        <ts-version:add-entity config-ref="config" key="id1">
            <ts-version:entity>
                <ts-version:entity key="Tittle">My Foo</ts-version:entity>
                <ts-version:entity key="Author">Foo</ts-version:entity>
            </ts-version:entity>
        </ts-version:add-entity>
    </flow>
</mule>
